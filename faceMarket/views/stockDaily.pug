extends header
block content
  .w3-container
      h1= answer["Meta Data"]["1. Information"]
      table.w3-table-all
        tr
          th Date
          th Open
          th High
          th Low
          th Close
          th Volume
        each val, index in answer["Time Series (Daily)"]
          tr
            td= index
            td= val["1. open"]
            td= val["2. high"]
            td= val["3. low"]
            td= val["4. close"]
            td= val["5. volume"]


      
         
           
